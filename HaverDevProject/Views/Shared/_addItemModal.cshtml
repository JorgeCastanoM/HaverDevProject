<!-- Item Modal-->
<div class="modal fade" id="addItemModal" tabindex="-1" role="dialog" aria-labelledby="ItemModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header bg-custom-color">
                <h4 class="modal-title" id="ItemmyModalLabel">Add New Item</h4>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            </div>
            <div class="modal-body px-4">
                @{
                    var item = new HaverDevProject.Models.Item();                    
                    <partial name="_ItemCreate" model="item" />
                }
            </div>
        </div>
    </div>
</div>